clust.centroid = function(i, dat, clusters) {
ind = (clusters == i)
colMeans(dat[ind,])
}
clust.sd = function(i, dat, clusters) {
ind = (clusters == i)
apply(dat[ind,],2,sd)
}
plot_ward = function(data){
d <- dist(data, method = "euclidean")
hc <- hclust(d^2, method="ward")
plot(hc)
list(hc = hc)
}
